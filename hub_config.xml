<?xml version="1.0" encoding="utf-8" ?>
<HarborBreezeHub>
    <!-- Initialize HB Hub Server -->
    <HttpsApiServer name="HBHubServer1" port="8080" certificate="./cert.crt" private_key="./private_key.key" />

    <!-- Initialize rf modules; Use BCM GPIO pin numbers -->
    <RFReceiver name="RFReceiver1" pin="16" />
    <RFTransmitter name="RFTransmitter1" pin="21" />

    <fans>
        <fan name="ExampleFan" remoteId="SSLLSLSSLLSSLSLLSSLLSSLSLSLLSL" />
    </fans>
</HarborBreezeHub>